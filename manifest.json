{
  "name": "Chrome Form QuickSubmit",
  "manifest_version": 2,
  "version": "0.5",
   "offline_enabled": true,
  "description": "Submit the currently-active form with alt+shift+s.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["quickSubmit.js"]
    }
  ],
  "permissions": [
    "<all_urls>", "tabs"
  ],
  "icons": { "16": "check-form-16.png",
           "48": "check-form-48.png",
          "128": "check-form-128.png",
          "512": "check-form-512.png" }
}
